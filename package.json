{
  "name": "facebook-events-ical-converter",
  "version": "1.1.11",
  "description": "App that converts events on Facebook event page to iCal file.",
  "main": "lib/index.js",
  "engines": {
    "npm": "6.11.3",
    "node": "10.15.0"
  },
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "start": "npm run build && node lib/index.js",
    "start:dev": "concurrently \"npm run build:dev\" \"NODE_ENV=development PORT=3000 nodemon lib/index.js\"",
    "start:dev:inspect": "npm run build:dev && NODE_ENV=development PORT=3000 nodemon --inspect lib/index.js",
    "test": "jest"
  },
  "keywords": [
    "facebook",
    "event",
    "ical",
    "convert",
    "download"
  ],
  "author": "Ondrej Synacek <ondrejsynacek@fastmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "dayjs": "^1.8.16",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-winston": "^4.0.1",
    "ics": "^2.18.0",
    "request": "^2.88.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-sinon": "^2.8.1",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.3",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "nodemon": "^1.19.3",
    "sinon": "^9.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "workbox-webpack-plugin": "^5.1.3"
  }
}
